    "import os\n",
    "\n",
    "logs_dir = \"logs\"\n",
    "os.makedirs(logs_dir, exist_ok=True)\n",
    "\n",
    "gitkeep_path = os.path.join(logs_dir, \".gitkeep\")\n",
    "\n",
    "with open(gitkeep_path, \"w\") as f:\n",
    "    f.write(\"\")  # create empty file\n",
    "\n",
    "print(\"Created\", gitkeep_path)\n"
   ]
